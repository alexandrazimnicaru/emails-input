{
  "name": "email-editor",
  "version": "1.0.0",
  "description": "Email editor library that adds & removes emails to a list",
  "main": "index.js",
  "author": "Alexandra Zimnicaru",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "jasmine-core": "^3.5.0",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-parcel": "^0.5.2",
    "node-sass": "^4.14.1",
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "clear-build-cache": "rm -rf .cache/ dist/",
    "start": "parcel tsc src/index.html",
    "prebuild": "rm -rf dist",
